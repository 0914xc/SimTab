{
    "name": "SimTab 新标签页",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "An extension that provides a new tab page.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "explore16.png",
        "32": "explore32.png",
        "48": "explore48.png",
        "128": "explore128.png"
    },
    "content_security_policy": "script-src-elem 'self' 'unsafe-eval' http://suggestion.baidu.com; object-src 'self'; img-src chrome://favicon;"
}
